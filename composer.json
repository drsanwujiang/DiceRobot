{
  "name": "drsanwujiang/dicerobot",
  "type": "project",
  "description": "A TRPG dice robot based on Swoole.",
  "homepage": "https://tool.drsanwujiang.com/dicerobot/",
  "license": "MIT",
  "authors": [
    {
      "name": "Drsanwujiang",
      "email": "drsanwujiang@gmail.com",
      "homepage": "https://www.drsanwujiang.com/"
    }
  ],
  "support": {
    "docs": "https://docs.drsanwujiang.com/dicerobot/",
    "issues": "https://github.com/drsanwujiang/DiceRobot/issues",
    "source": "https://github.com/drsanwujiang/DiceRobot/"
  },
  "require": {
    "php": "^7.4",
    "ext-json": "*",
    "imefisto/psr-swoole-native": "^0.0.2",
    "monolog/monolog": "^2.1",
    "myclabs/php-enum": "^1.7",
    "php-di/php-di": "^6.2",
    "selective/array-reader": "^0.3.0",
    "selective/config": "^0.1.1",
    "swlib/saber": "^1.0"
  },
  "require-dev": {
    "swoole/ide-helper": "^4.5"
  },
  "autoload": {
    "psr-4": {
      "DiceRobot\\": "src"
    },
    "files": [
      "src/include/const.php"
    ]
  },
  "config": {
    "sort-packages": true
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  }
}
